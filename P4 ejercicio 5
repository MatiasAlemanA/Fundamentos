#include "math.h"

using namespace std;


void complemento(int);

int main(){
	int n;
	cout<<"ingrese u n numero"<<endl;
	cin>>n;
	complemento(n);
	getch();
return 0;
}

void complemento(int n){
	int d,comp;
	cout<<"el complemento es : ";
	while(n>=1){
	d=n%10;
	comp=9-d;
	
	cout<<comp;
	n=n/10;
	}



}
